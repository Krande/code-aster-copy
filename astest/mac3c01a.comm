DEBUT(CODE=_F(NIV_PUB_WEB="INTRANET"), IMPR_MACRO="NON")

MA1 = LIRE_MAILLAGE(FORMAT="MED", UNITE=20)

DATAMAC = LIRE_TABLE(UNITE=38, FORMAT="LIBRE", SEPARATEUR="\t")

RESU_C1 = CALC_MAC3COEUR(
    TYPE_COEUR="TEST",
    TABLE_N=DATAMAC,
    MAILLAGE_N=MA1,
    DEFORMATION=_F(UNITE_THYC=32, NIVE_FLUENCE=33),
)

t4 = 4.0e-3
t9 = 33 + 8.0e-3

TAB1 = CREA_TABLE(
    RESU=_F(
        RESULTAT=RESU_C1,
        NOM_CHAM="DEPL",
        INST=t4,
        NOM_CMP=("DX", "DY"),
        GROUP_MA=("CR_B_B", "TG_B_B"),
    )
)

TAB2 = CREA_TABLE(
    RESU=_F(
        RESULTAT=RESU_C1,
        NOM_CHAM="DEPL",
        INST=t4,
        NOM_CMP=("DX", "DY", "DZ"),
        GROUP_NO=("P_CUV", "FIX"),
    )
)

TAB3 = POST_MAC3COEUR(
    RESULTAT=RESU_C1,
    TYPE_COEUR="TEST",
    INST=t9,
    TABLE=DATAMAC,
    TYPE_CALCUL="DEFORMATION",
    OPERATION="EXTRACTION",
)

TEST_TABLE(
    VALE_CALC=-0.012372624393017367,
    NOM_PARA="DX",
    TABLE=TAB1,
    FILTRE=_F(NOM_PARA="NOEUD", VALE_K="N430"),
)

TEST_TABLE(
    VALE_CALC=0.0034123188141379004,
    NOM_PARA="DY",
    TABLE=TAB1,
    FILTRE=_F(NOM_PARA="NOEUD", VALE_K="N465"),
)

TEST_TABLE(VALE_CALC=-0.012248857521062499, NOM_PARA="DX", TYPE_TEST="MAX", TABLE=TAB1)

TEST_TABLE(VALE_CALC=0.0034442892645998244, NOM_PARA="DY", TYPE_TEST="MAX", TABLE=TAB1)

TEST_TABLE(
    VALE_CALC=-0.021929946270868632,
    NOM_PARA="DX",
    TABLE=TAB2,
    FILTRE=_F(NOM_PARA="NOEUD", VALE_K="N509"),
)

TEST_TABLE(
    VALE_CALC=0.0010398774297798892,
    NOM_PARA="DY",
    TABLE=TAB2,
    FILTRE=_F(NOM_PARA="NOEUD", VALE_K="N509"),
)

TEST_TABLE(
    VALE_CALC=-0.0005242425230768122,
    NOM_PARA="DZ",
    TABLE=TAB2,
    FILTRE=_F(NOM_PARA="NOEUD", VALE_K="N509"),
)

TEST_TABLE(
    VALE_CALC=3.1827111432095183,
    NOM_PARA="Ro",
    TABLE=TAB3,
    FILTRE=_F(NOM_PARA="TEST", VALE_K="B02"),
)

TEST_TABLE(VALE_CALC_K="2C", NOM_PARA="Forme", TABLE=TAB3, FILTRE=_F(NOM_PARA="TEST", VALE_K="B02"))

FIN()
