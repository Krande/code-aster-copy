chPostRocheTout_SPG = POST_ROCHE(ZONE_ANALYSE=(_F(GROUP_MA='RUN',
                                               GROUP_NO_ORIG = 'A',
                                              ),
                                            _F(GROUP_MA='BRANCH',
                                               GROUP_NO_ORIG = 'C',
                                              ),
                                  ),
                    MODELE = modele,
                    CARA_ELEM=cara_elem,
                    CHAM_MATER=ch_mater, 
                    RESU_MECA=(
                               _F(TYPE_CHAR='POIDS',
                                 RESULTAT=poids_propre,
                                 NUME_ORDRE=1,
                                 ),
                                _F(TYPE_CHAR='SISM_INER_SPEC',
                                 RESULTAT=sismique,
