chPostRocheTout_SP = POST_ROCHE(ZONE_ANALYSE=(_F(GROUP_MA='RUN',
                                               GROUP_NO_ORIG = 'A',
                                              ),
                                            _F(GROUP_MA='BRANCH',
                                               GROUP_NO_ORIG = 'C',
                                              ),
                                  ),
                    MODELE = modele,
                    CARA_ELEM=cara_elem,
