# 7- seisme inertiel + deplacement (SD)
chPostRocheTout_SD = POST_ROCHE(ZONE_ANALYSE=(_F(GROUP_MA='RUN',
                                               GROUP_NO_ORIG = 'A',
                                              ),
                                            _F(GROUP_MA='BRANCH',
                                               GROUP_NO_ORIG = 'C',
                                              ),
                                  ),
                    MODELE = modele,
                    CARA_ELEM=cara_elem,
                    CHAM_MATER=ch_mater, 
                    RESU_MECA=(

                               _F(TYPE_CHAR='DEPLACEMENT',
                                 RESULTAT=deplacement_impose,
