DATAMAC = LIRE_TABLE(UNITE=38, FORMAT="LIBRE", SEPARATEUR="\t")

RESU = CALC_MAC3COEUR(
    TYPE_COEUR="TEST",
    TABLE_N=DATAMAC,
    MAILLAGE_N=MA1,
    TYPE_DEFORMATION="GROT_GDEP",
    DEFORMATION=_F(UNITE_THYC=32, TEMP_IMPO=300.0, NIVE_FLUENCE=33),
)

t4 = 4.0e-3

TAB = CREA_TABLE(
    RESU=_F(
        RESULTAT=RESU, NOM_CHAM="DEPL", INST=t4, NOM_CMP=("DX", "DY"), GROUP_MA=("CR_B_B", "TG_B_B")
    )
)

TEST_TABLE(
    VALE_CALC=-0.012494847420171493,
    NOM_PARA="DX",
    TABLE=TAB,
    FILTRE=_F(NOM_PARA="NOEUD", VALE_K="N430"),
)

TEST_TABLE(VALE_CALC=-0.012370644628782694, NOM_PARA="DX", TYPE_TEST="MAX", TABLE=TAB)

FIN()
